    "import numpy as np\n",
    "import numpy.fft as fft\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "N=np.power(2,16)\n",
    "\n",
    "Fx = 500\n",
    "Tx = 1/Fx\n",
    "\n",
    "t = np.linspace(0, 10*Tx, N)\n",
    "\n",
    "\n",
    "x = np.sin(2*np.pi*Fx*t)\n",
    "X = fft.fft(x)\n",
    "X = X[0:10]\n",
    "f = np.linspace(0, 9, 10)\n",
    "\n",
    "\n",
    "fig_1 = plt.figure(figsize=(14,7))\n",
    "\n",
    "plt.subplot(1,2,1)\n",
    "plt.title(\"$sin(2\\pi{200*10^3}t)$\");\n",
    "plt.plot(t,x)\n",
    "\n",
    "plt.subplot(1,2,2)\n",
    "plt.plot(f,np.abs(X))\n",
    "plt.show()"
